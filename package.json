{
  "name": "vuetify-pro-dialog",
  "version": "0.2.0",
  "private": false,
  "license": "MIT",
  "description": "vuetify-pro-dialog",
  "main": "dist/vuetify-pro-dialog.umd.js",
  "module": "dist/vuetify-pro-dialog.esm.js",
  "unpkg": "dist/vuetify-pro-dialog.min.js",
  "types": "types/index.d.ts",
  "author": "yikoyu<2282373181@qq.com>",
  "repository": {
    "type": "git",
    "url": "https://gitee.com/hzsage/vuetify-pro-dialog"
  },
  "files": [
    "dist",
    "types",
    "package.json",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config rollup.config.js --format umd --file dist/vuetify-pro-dialog.umd.js",
    "build:es": "rollup --config rollup.config.js --format es --file dist/vuetify-pro-dialog.esm.js",
    "build:unpkg": "rollup --config rollup.config.js --format iife --file dist/vuetify-pro-dialog.min.js",
    "lint": "vue-cli-service lint",
    "lint:nofix": "vue-cli-service lint --no-fix",
    "push": "npm run build && npm publish --registry http://registry.npmjs.org"
  },
  "keywords": [
    "vue",
    "typescript",
    "vuetify",
    "dialog"
  ],
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@rollup/plugin-buble": "^0.21.3",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-image": "^2.0.6",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@types/core-js": "^2.5.4",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "core-js": "^3.8.2",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "4.2.5",
    "lint-staged": "^11.0.0",
    "prettier": "^1.19.1",
    "rollup": "^2.47.0",
    "rollup-plugin-css-porter": "^1.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "rollup-plugin-vue": "5.1.9",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "typescript": "~3.9.3",
    "vue": "^2.6.12",
    "vue-cli-plugin-vuetify": "^2.0.9",
    "vue-template-compiler": "^2.6.11",
    "vuetify": "^2.4.2",
    "vuetify-loader": "^1.6.0"
  }
